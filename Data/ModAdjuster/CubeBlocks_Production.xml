<?xml version="1.0" encoding="utf-8"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<CubeBlocks>
		<!-- Assemblers also dont work with mod adjuster, blueprints from other mods dont get applied correctly -->
		<Definition xsi:type="MyObjectBuilder_RefineryDefinition">
			<Id>
				<TypeId>Refinery</TypeId>
				<SubtypeId>LargePrototechRefinery</SubtypeId>
			</Id>
			<DisplayName>Advanced Refinery</DisplayName>
			<Components>
                <Component Subtype="SteelPlate" Count="1100" />
				<Component Subtype="PrototechCircuitry" Count="1"/>
				<Component Subtype="PrototechPropulsionUnit" Count="20"/>
                <Component Subtype="Construction" Count="40" />
                <Component Subtype="LargeTube" Count="20" />
                <Component Subtype="Motor" Count="16" />
                <Component Subtype="MetalGrid" Count="20" />
                <Component Subtype="Computer" Count="20" />
                <Component Subtype="SteelPlate" Count="100" />
			</Components>
			<CriticalComponent Subtype="Computer" Index="0"/>
            <OperationalPowerConsumption>2</OperationalPowerConsumption>
            <RefineSpeed>8</RefineSpeed>
            <MaterialEfficiency>2.0</MaterialEfficiency>
		</Definition>
		<Definition xsi:type="MyObjectBuilder_RefineryDefinition">
			<Id>
				<TypeId>Refinery</TypeId>
				<SubtypeId>SmallPrototechRefinery</SubtypeId>
			</Id>
			<DisplayName>Advanced Refinery</DisplayName>
			<Components>
                <Component Subtype="SteelPlate" Count="220" />
				<Component Subtype="PrototechPropulsionUnit" Count="5"/>
                <Component Subtype="Construction" Count="8" />
                <Component Subtype="LargeTube" Count="4" />
                <Component Subtype="Motor" Count="3" />
                <Component Subtype="MetalGrid" Count="4" />
                <Component Subtype="Computer" Count="4" />
                <Component Subtype="SteelPlate" Count="20" />
			</Components>
			<CriticalComponent Subtype="Computer" Index="0"/>
		</Definition>
		<!-- increase speed of O2 Generators -->
        <Definition xsi:type="MyObjectBuilder_OxygenGeneratorDefinition">
            <Id>
                <TypeId>OxygenGenerator</TypeId>
                <SubtypeId></SubtypeId>
            </Id>
            <IceConsumptionPerSecond>400</IceConsumptionPerSecond> <!-- vanilla = 25 -->
            <OperationalPowerConsumption>8</OperationalPowerConsumption> <!-- vanilla = 0.5 -->
        </Definition>
        <Definition xsi:type="MyObjectBuilder_OxygenGeneratorDefinition">
            <Id>
                <TypeId>OxygenGenerator</TypeId>
                <SubtypeId>OxygenGeneratorSmall</SubtypeId>
            </Id>
            <IceConsumptionPerSecond>40</IceConsumptionPerSecond> <!-- vanilla = 5 -->
            <OperationalPowerConsumption>0.8</OperationalPowerConsumption> <!-- vanilla = 0.1 -->
        </Definition>
	</CubeBlocks>
</Definitions>
